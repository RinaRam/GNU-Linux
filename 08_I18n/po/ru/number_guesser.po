#: src/number_guesser.c:22
#, c-format
msgid "Guess a number between 1 and 100.\n"
msgstr "Загадайте число от 1 до 100.\n"

#: src/number_guesser.c:27
#, c-format
msgid "Is your number greater than %d? (yes/no): "
msgstr "Ваше число больше %d? (да/нет): "

#: src/number_guesser.c:30
msgid "yes"
msgstr "да"

#: src/number_guesser.c:32
msgid "no"
msgstr "нет"

#: src/number_guesser.c:35
#, c-format
msgid "Incorrect answer. Please answer 'yes' or 'no'.\n"
msgstr "Некорректный ответ. Пожалуйста, отвечайте 'да' или 'нет'.\n"

#: src/number_guesser.c:40
#, c-format
msgid "You made a mistake or guessed a number outside the range of 1 to 100.\n"
msgstr "Вы ошиблись либо загадали число вне диапазона от 1 до 100.\n"

#: src/number_guesser.c:45
#, c-format
msgid "Your number is: %d\n"
msgstr "Ваше число: %d\n"
